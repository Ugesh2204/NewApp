@model GymManagementSystem.Entities.Models.Employee
@{
    ViewData["Title"] = "CreateEmployee";
    Layout = null;
}

<h1>CreateEmployee</h1>




    <div class="modal-dialog" role="document" id="addEmployee">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle">Modal title</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form id="createStaff">
                    <label>EmployeeName</label>
                    <input name="EmployeeName" />
                    <br />
                    <label>Description</label>
                    <input name="Description" />
                    <br />

                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary" data-save="modal" id="savebtn">Save changes</button>
                </form>
            </div>
          
        </div>
    </div>





